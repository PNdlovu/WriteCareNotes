{
  "name": "writecarenotes-ai-enhanced",
  "version": "1.0.0",
  "description": "Enterprise-grade healthcare management system with AI enhancements",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
        "build": "npm run compliance:check && npx tsc --project tsconfig.core.json",
    "build:full": "npx tsc",
    "build:production": "npm run compliance:check && npx tsc --project tsconfig.core.json --outDir dist/core",
    "build:unsafe": "tsc --project tsconfig.json",
    "dev": "ts-node src/server.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "deploy": "./deploy.sh",
    "compliance:check": "ts-node scripts/compliance-checker.ts",
    "compliance:fix": "ts-node scripts/compliance-fixer.ts",
    "compliance:report": "npm run compliance:check && start reports/british-isles-compliance-report.md",
    "compliance:ci": "ts-node scripts/compliance-checker.ts --ci"
  },
  "keywords": [
    "healthcare",
    "ai",
    "care-home",
    "medication",
    "compliance",
    "voice-assistant",
    "predictive-analytics"
  ],
  "author": "WriteCareNotes Team",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/testing": "^11.1.6",
    "@nestjs/typeorm": "^11.0.0",
    "@types/axios": "^0.9.36",
    "@types/commander": "^2.12.0",
    "@types/decimal.js": "^0.0.32",
    "@types/eventemitter2": "^2.2.1",
    "@types/joi": "^17.2.2",
    "@types/morgan": "^1.9.10",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@types/stripe": "^8.0.416",
    "@types/uuid": "^10.0.0",
    "axios": "^1.12.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "clsx": "^2.1.1",
    "commander": "^14.0.1",
    "cors": "^2.8.5",
    "decimal.js": "^10.6.0",
    "dotenv": "^17.2.2",
    "eventemitter2": "^6.4.9",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "joi": "^18.0.1",
    "lucide-react": "^0.544.0",
    "morgan": "^1.10.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "stripe": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "typeorm": "^0.3.27",
    "typescript": "^5.0.0",
    "uuid": "^13.0.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@nestjs/core": "^11.1.6",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.19.19",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}

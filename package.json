{
  "name": "writecarenotes-ai-enhanced",
  "version": "1.0.0",
  "description": "Enterprise-grade healthcare management system with AI enhancements",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "npm run compliance:check && npx tsc --project tsconfig.core.json",
    "build:full": "npx tsc",
    "build:production": "npm run compliance:check && npx tsc --project tsconfig.core.json --outDir dist/core",
    "build:unsafe": "tsc --project tsconfig.json",
    "dev": "ts-node src/server.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "deploy": "./deploy.sh",
    "compliance:check": "ts-node scripts/compliance-checker.ts",
    "compliance:fix": "ts-node scripts/compliance-fixer.ts",
    "compliance:report": "npm run compliance:check && start reports/british-isles-compliance-report.md",
    "compliance:ci": "ts-node scripts/compliance-checker.ts --ci",
    "completion:scan": "ts-node scripts/completion-agent.ts",
    "completion:force": "ts-node scripts/completion-agent.ts --force-fix",
    "completion:report": "ts-node scripts/completion-agent.ts && start MODULE_CLOSURE_TRACKER.md",
    "enterprise:validate": "npm run compliance:check && npm run completion:scan && npm run build",
    "enterprise:deploy": "npm run enterprise:validate && npm run build:production",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm migration:generate -- -d src/config/typeorm.config.ts",
    "migration:create": "npm run typeorm migration:create",
    "migration:run": "npm run typeorm migration:run -- -d src/config/typeorm.config.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d src/config/typeorm.config.ts",
    "migration:show": "npm run typeorm migration:show -- -d src/config/typeorm.config.ts",
    "schema:sync": "npm run typeorm schema:sync -- -d src/config/typeorm.config.ts",
    "schema:drop": "npm run typeorm schema:drop -- -d src/config/typeorm.config.ts"
  },
  "keywords": [
    "healthcare",
    "ai",
    "care-home",
    "medication",
    "compliance",
    "voice-assistant",
    "predictive-analytics"
  ],
  "author": "WriteCareNotes Team",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^4.0.2",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/testing": "^11.1.6",
    "@nestjs/typeorm": "^11.0.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@types/axios": "^0.9.36",
    "@types/bcrypt": "^6.0.0",
    "@types/commander": "^2.12.0",
    "@types/decimal.js": "^0.0.32",
    "@types/eventemitter2": "^2.2.1",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@types/socket.io": "^3.0.1",
    "@types/stripe": "^8.0.416",
    "@types/uuid": "^9.0.7",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "clsx": "^2.1.1",
    "commander": "^14.0.1",
    "cors": "^2.8.5",
    "decimal.js": "^10.6.0",
    "dotenv": "^17.2.2",
    "eventemitter2": "^6.4.9",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "firebase-admin": "^13.5.0",
    "helmet": "^7.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.544.0",
    "morgan": "^1.10.1",
    "nodemailer": "^7.0.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "socket.io": "^4.8.1",
    "stripe": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "twilio": "^5.10.2",
    "typeorm": "^0.3.27",
    "uuid": "^9.0.1",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@nestjs/core": "^11.1.6",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.0.0",
    "@types/knex": "^0.15.2",
    "@types/node": "^20.19.19",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "glob": "^11.0.3",
    "jest": "^29.0.0",
    "knex": "^3.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}

{
  "name": "writecarenotes-mobile",
  "version": "1.0.0",
  "description": "WriteCareNotes Mobile App - Healthcare Management for British Isles Care Homes",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "react-native-web start",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace WriteCareNotesMobile.xcworkspace -scheme WriteCareNotesMobile -configuration Release -destination generic/platform=iOS -archivePath WriteCareNotesMobile.xcarchive archive",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "postinstall": "cd ios && pod install",
    "clean": "react-native clean-project-auto",
    "check-fake": "node scripts/check-fake-implementations.js",
    "compliance:check": "node scripts/compliance-check.js",
    "security:audit": "npm audit && node scripts/security-audit.js"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.2",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "react-native-screens": "^3.29.0",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-reanimated": "^3.6.2",
    "react-native-vector-icons": "^10.0.3",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/netinfo": "^11.2.1",
    "react-native-keychain": "^8.1.3",
    "react-native-biometrics": "^3.0.1",
    "react-native-device-info": "^10.11.0",
    "react-native-permissions": "^4.1.1",
    "react-native-camera": "^4.2.1",
    "react-native-image-picker": "^7.1.0",
    "react-native-document-picker": "^9.1.1",
    "react-native-pdf": "^6.7.3",
    "react-native-print": "^0.8.0",
    "react-native-share": "^10.0.2",
    "react-native-qrcode-scanner": "^1.5.5",
    "react-native-push-notification": "^8.1.1",
    "@react-native-firebase/app": "^19.0.1",
    "@react-native-firebase/messaging": "^19.0.1",
    "@react-native-firebase/analytics": "^19.0.1",
    "@react-native-firebase/crashlytics": "^19.0.1",
    "react-redux": "^8.1.3",
    "@reduxjs/toolkit": "^1.9.7",
    "redux-persist": "^6.0.0",
    "react-query": "^3.39.3",
    "axios": "^1.6.2",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "crypto-js": "^4.2.0",
    "react-hook-form": "^7.48.2",
    "yup": "^1.4.0",
    "@hookform/resolvers": "^3.3.2",
    "react-native-chart-kit": "^6.12.0",
    "react-native-svg": "^14.1.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-modal": "^13.0.1",
    "react-native-swipe-gestures": "^1.0.5",
    "react-native-calendar-picker": "^7.1.4",
    "react-native-picker-select": "^9.0.1",
    "react-native-search-filter": "^0.1.5",
    "react-native-super-grid": "^4.9.2",
